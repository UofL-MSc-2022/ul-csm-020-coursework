			"email": "jek9@student.london.ac.uk"
		},
		"version": "1.0.0"
	},
	"paths": {
		"/api/version": {
			"get": {
				"summary": "Get MiniWall API version.",
				"produces": "application/json",
				"responses": {
					"200": {
						"description": "OK"
					}
				}
			}
		},
		"/api/user/register": {
			"post": {
				"summary": "Register new user",
				"consumes": "application/json",
				"parameters": [
					{
						"in": "body",
						"schema": {"$ref": "#/definitions/User"}
					}
				],
				"produces": "application/json",
				"responses": {
					"200": {
						"description": "OK"
					},
					"400": {
						"description": "Invalid"
					}
				}
			}
		}
	},
	"definitions": {
		"User": {
			"type": "object",
			"required": ["screen_name", "email", "password"],
			"properties": {
				"screen_name": {"type": "string"},
				"email": {"type": "string"},
				"password": {"type": "string"}
			}
		}
	}
}
